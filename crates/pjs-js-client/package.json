{
  "name": "@pjs/client",
  "version": "0.3.0-alpha.1",
  "description": "Priority JSON Streaming Protocol (PJS) client library for JavaScript/TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "dev": "concurrently \"npm run build:watch\" \"npm run test:watch\"",
    "prepublishOnly": "npm run build && npm run test",
    "docs": "typedoc src/index.ts"
  },
  "keywords": [
    "json",
    "streaming",
    "priority",
    "performance",
    "progressive-loading",
    "pjs",
    "websocket",
    "real-time"
  ],
  "author": "PJS Contributors",
  "license": "MIT OR Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/pjs-rs/pjs",
    "directory": "crates/pjs-js-client"
  },
  "bugs": {
    "url": "https://github.com/pjs-rs/pjs/issues"
  },
  "homepage": "https://github.com/pjs-rs/pjs#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@jest/globals": "^30.0",
    "@rollup/plugin-node-resolve": "^16.0",
    "@rollup/plugin-typescript": "^12.0",
    "@types/jest": "^30.0",
    "@types/ws": "^8.18",
    "@typescript-eslint/eslint-plugin": "^8.54",
    "@typescript-eslint/parser": "^8.54",
    "concurrently": "^9.2",
    "eslint": "^9.39",
    "jest": "^30.2",
    "jest-environment-jsdom": "^30.2",
    "rollup": "^4.57",
    "ts-jest": "^29.4",
    "tslib": "^2.8",
    "typedoc": "^0.28",
    "typescript": "^5.6"
  },
  "dependencies": {
    "ws": "^8.19.0"
  },
  "optionalDependencies": {
    "pjs-wasm": "file:../pjs-wasm/pkg"
  },
  "peerDependencies": {
    "ws": "^8.16.0"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
