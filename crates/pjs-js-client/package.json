{
  "name": "@pjs/client",
  "version": "0.3.0-alpha.1",
  "description": "Priority JSON Streaming Protocol (PJS) client library for JavaScript/TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.esm.js", 
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "dev": "concurrently \"npm run build:watch\" \"npm run test:watch\"",
    "prepublishOnly": "npm run build && npm run test",
    "docs": "typedoc src/index.ts"
  },
  "keywords": [
    "json",
    "streaming", 
    "priority",
    "performance",
    "progressive-loading",
    "pjs",
    "websocket",
    "real-time"
  ],
  "author": "PJS Contributors",
  "license": "MIT OR Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/pjs-rs/pjs",
    "directory": "crates/pjs-js-client"
  },
  "bugs": {
    "url": "https://github.com/pjs-rs/pjs/issues"
  },
  "homepage": "https://github.com/pjs-rs/pjs#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^4.12.1",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.12",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "ws": "^8.16.0"
  },
  "peerDependencies": {
    "ws": "^8.16.0"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}